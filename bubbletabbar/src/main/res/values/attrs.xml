<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleTabBar">
        <attr name="bubbletab_menuResource" format="reference"/>
        <attr name="bubbletab_selected_color" format="color"/>
        <attr name="bubbletab_unselected_color" format="color"/>
        <attr name="bubbletab_icon" format="integer"/>
        <attr name="bubbletab_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="Bubble">
        <attr name="android:id"/>
        <attr name="android:title"/>
        <attr name="android:icon"/>
        <attr name="android:enabled"/>
        <attr name="android:checked"/>
        <attr name="android:iconTint" format="color"/>
    </declare-styleable>
</resources>